{% extends 'base.html.twig' %}

{% block title %}
	Modifier une catégorie
{% endblock %}

{% block body %}
	<h1>Modification de la catégorie
		{{ category.name }}
	</h1>
	{{form_start(formView)}}

	{{form_widget(formView)}}

	<button type="submit" class="btn btn-primary">
		<i class="fas fa-save"></i>
		Enregistrer les Changements
	</button>
	{{ form_end(formView) }}
{% endblock %}
